{
  "compilerOptions": {
    "outDir": "./dist/",
    "typeRoots": [
      "src/types",
      "./node_modules/vue/types"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@popup/*": [
        "src/popup/*"
      ],
      "@content/*": [
        "src/content/*"
      ]
    },
  },
  "include": [
    "src"
, "types"  ],
  "exclude": [
    "node_modules"
  ]
}